    "def moving_average(a, n=3) :\n",
    "    ret = np.cumsum(a,dtype=float)\n",
    "    ret[n:] = ret[n:] - ret[:-n]\n",
    "    return ret[n - 1:] / n\n",
    "a = [3, 5, 7, 2, 8, 10, 11, 65, 72, 81, 99, 100, 150]\n",
    "moving_average(a)\n",
    "\n",
    "moving_average(a, n=3)"
   ]
